{
  "name": "joi-to-swagger",
  "version": "6.2.0",
  "description": "Conversion library for transforming joi schema objects into swagger / OpenApi OAS 3.1 schema definitions.",
  "keywords": [
    "joi",
    "swagger",
    "schema",
    "api",
    "convert",
    "openapi"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoodRequest/joi-to-swagger.git"
  },
  "license": "MIT",
  "author": "GoodRequest",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "lint": "eslint ./",
    "test": "tap tests.js",
    "test:ci": "tap --reporter=tap tests.js",
    "test:coverage:html": "tap --coverage-report=html tests.js",
    "test:debug": "tap --node-arg=--inspect --no-timeout tests.js"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@joi/date": "^2.1.0",
    "eslint": "^7.32.0",
    "eslint-config-twipped": "~3.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "joi": "^17.7.0",
    "tap": "^16.3.2",
    "tapsuite": "^2.0.1"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
